{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run pytk_publisher_tool",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/tools/pytk_publisher_tool.py",
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "Run Python HTTP Server",
            "type": "debugpy",
            "request": "launch",
            "module": "http.server",
            "args": ["8000"],
            "console": "integratedTerminal",
            "justMyCode": false,
            "cwd": "${workspaceFolder}"
        }
    ]
}
